@{
    ViewBag.Title = "_ltrule";
}
<table class="con_table table_rule" cellspacing="0" style="border-collapse: collapse; border: 1px solid #cacaca; margin-top: 10px;" bgcolor="#CACACA">
    <tbody>
   
    @if (ViewData["ltruletype"].ToString() == "4" || ViewData["ltruletype"].ToString() == "1" || ViewData["ltruletype"].ToString() == "9")
    {
        <tr><td colspan="4">采用北京福彩中心快乐8数据，与PC蛋蛋结果相同，每5分钟一期，每天179期，每天0-9点暂停开奖</td></tr>
        <tr>
            <td width="120">如第654574期</td>
            <td colspan="3">02,04,07,08,18,22,25,30,35,36,43,49,50,53,59,66,69,71,74,75</td>

        </tr>
        <tr>
            <td width="120">区位</td>
            <td>第一区[第1/2/3/4/5/6位数字]</td>
            <td>第二区[第7/8/9/10/11/12位数字]</td>
            <td>第三区[第13/14/15/16/17/18位数字]</td>

        </tr>
        <tr>
            <td>数字</td>
            <td>02,04,07,08,18,22</td>
            <td>25,30,35,36,43,49</td>
            <td>50,53,59,66,69,71</td>

        </tr>
        <tr>
            <td>求和</td>
            <td>61</td>
            <td>218</td>
            <td>368</td>

        </tr>
        <tr>
            <td>计算</td>
            <td>取尾数</td>
            <td>取尾数</td>
            <td>取尾数</td>

        </tr>
        <tr>
            <td>结果</td>
            <td><li class="finalbig">1</li></td>
            <td><li class="finalbig">8</li></td>
            <td><li class="finalbig">8</li></td>

        </tr>
        <tr>
            <td>开奖</td>
            <td colspan="3"><li class="finalbig">1</li> + <li class="finalbig">8</li> + <li class="finalbig">8</li> = <li class="finalbig">
                @(ViewData["ltruletype"].ToString() == "4" ? "对" :  "17")
            </li></td>

        </tr>
    }else if (ViewData["ltruletype"].ToString() == "2" || ViewData["ltruletype"].ToString() == "5" || ViewData["ltruletype"].ToString() == "6")
    {
        if (ViewData["ltruletype"].ToString() == "6")
        {
            <tr><td colspan="4"> 采用韩国快乐8数据，每1分半钟一期，每天5:00-07:00暂停开奖</td></tr>
        }
        else
        {
             <tr><td colspan="4"> 采用加拿大快乐8数据，每4分钟一期，每天336期，每天19:00-20:30暂停开奖</td></tr>
        }
        <tr>
            <td width="120">如第1234567期</td>
            <td colspan="3">01,06,07,13,15,28,31,32,33,35,40,43,44,53,57,62,64,65,68,80(从小到大依次排列)</td>

        </tr>
        <tr>
            <td>区位</td>
            <td>第一区[第2/5/8/11/14/17位数字]</td>
            <td>第二区[第3/6/9/12/15/18位数字]</td>
            <td>第三区[第4/7/10/13/16/19位数字]</td>

        </tr>
        <tr>
            <td>数字</td>
            <td>06,15,32,40,53,64</td>
            <td>07,28,33,43,57,65</td>
            <td>13,31,35,44,62,68</td>

        </tr>
        <tr>
            <td>求和</td>
            <td>210</td>
            <td>233</td>
            <td>253</td>

        </tr>
        <tr>
            <td>计算</td>
            <td>取尾数</td>
            <td>取尾数</td>
            <td>取尾数</td>

        </tr>
        <tr>
            <td>结果</td>
            <td><li class="finalbig">0</li></td>
            <td><li class="finalbig">3</li></td>
            <td><li class="finalbig">3</li></td>

        </tr>
        <tr>
            <td>开奖</td>
            <td colspan="3"><li class="finalbig">0</li> + <li class="finalbig">3</li> + <li class="finalbig">3</li> = <li class="finalbig"> @(ViewData["ltruletype"].ToString() == "5" ? "对" : "6")</li></td>

        </tr>
    }
    else if (ViewData["ltruletype"].ToString() == "7" || ViewData["ltruletype"].ToString() == "8" || ViewData["ltruletype"].ToString() == "10")
    {
        <tr><td colspan="2">
            @if (ViewData["ltruletype"].ToString() != "10")
            {
                @: 采用北京福彩中心PK10数据，每5分钟一期，每天179期，每天0 - 9点暂停开奖
            }
            else
            {
                @: 计算机系统随机产生10个数字,数字范围1-10各不相同,每2分钟一期，24小时开奖
            }
        </td></tr>
		<tr>
			<td width="120">如第449159期</td>
			<td><em class="regular06"></em>
				<em class="regular05"></em>
				<em class="regular01"></em>
				<em class="regular03"></em>
				<em class="regular04"></em>
				<em class="regular10"></em>
				<em class="regular07"></em>
				<em class="regular08"></em>
				<em class="regular09"></em>
				<em class="regular02"></em></td>
        </tr>
		<tr>
			<td>计算</td>
		    <td colspan="3">
		        @(ViewData["ltruletype"].ToString() == "8" ?"取开奖号码前两位之和":"取开奖号码首位")
		    </td>
	
        </tr>
		<tr>
			<td>开奖</td>
		    <td colspan="3">
		          @(ViewData["ltruletype"].ToString() == "8" ?"<em class='regular06'></em> + <em class='regular05'></em> = <li class='finalbig'>11</li>":"<li class='finalbig'>6</li>")
		    </td>
	
        </tr>
    }
    else if (ViewData["ltruletype"].ToString() == "3")
    {
        <tr><td colspan="4">计算机系统随机产生3个数字,每个数字范围0-9,每2分钟一期，24小时开奖</td></tr>
        <tr>
            <td width="120">如第449159期</td>
            <td>
                0-9随机数
            </td>
            <td>
                0-9随机数
            </td>
            <td>
                0-9随机数
            </td>
        </tr>
        <tr>
            <td>结果</td>
            <td>
                <li class="finalbig">3</li>
            </td>
            <td>
                <li class="finalbig">6</li>
            </td>
            <td>
                <li class="finalbig">1</li>
            </td>

        </tr>
        <tr>
            <td>开奖</td>
            <td colspan="3"><li class="finalbig">3</li> + <li class="finalbig">6</li> + <li class="finalbig">1</li> = <li class="finalbig">10</li></td>
        </tr>
    }
    else if (ViewData["ltruletype"].ToString() == "11" || ViewData["ltruletype"].ToString() == "12")
    {
        <tr><td colspan="@(ViewData["ltruletype"].ToString()=="12"?"4":"3")">计算机系统随机产生@(ViewData["ltruletype"].ToString()=="12"?"3":"2")个数字,每个数字范围1-6,每2分钟一期，24小时开奖</td></tr>
         <tr>
            <td width="120">如第449159期</td>
            <td>
                1-6随机数
            </td>
            <td>
                1-6随机数
            </td>
            <td style="@(ViewData["ltruletype"].ToString()=="12"?"":"display:none;")">
                1-6随机数
            </td>
        </tr>
        <tr>
            <td>结果</td>
            <td>
                <li class="finalbig">3</li>
            </td>
            <td>
                <li class="finalbig">6</li>
            </td>
            <td style="@(ViewData["ltruletype"].ToString()=="12"?"":"display:none;")">
                <li class="finalbig">1</li>
            </td>

        </tr>
        <tr>
            <td>开奖</td>
            <td colspan="@(ViewData["ltruletype"].ToString()=="12"?"3":"4")"><li class="finalbig">3</li> + <li class="finalbig">6</li> @(ViewData["ltruletype"].ToString() == "12"?"+ <li class='finalbig'>1</li>":"") = <li class="finalbig"> @(ViewData["ltruletype"].ToString()=="12"?"10":"9")</li></td>
        </tr>
    }

    @if (ViewData["ltruletype"].ToString() == "4" || ViewData["ltruletype"].ToString() == "5")
    {
        <tr>
            <td colspan="4" style="color: #F90; font-weight: bold">游戏结果说明</td>
        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;结果优先顺序：豹 &gt; 顺 &gt; 对 &gt; 半 &gt; 杂</td>

        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;<li class="finalbig">豹</li> 3个结果号码相同，如222,333,999</td>

        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;<li class="finalbig">顺</li> 3个结果号码从小到大排序后，号码都相连，如231,765,645.特例:排序后019算顺子</td>

        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;<li class="finalbig">对</li> 3个结果号码只有两个相同，如535,337,899</td>

        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;<li class="finalbig">半</li> 3个结果号码只有任意两个是相连的,不包含顺、对，如635,367,874.特例:包含0和9也算顺子</td>

        </tr>
        <tr>
            <td colspan="4" style="text-align: left">&nbsp;<li class="finalbig">杂</li> 3个结果号码没有任何关联，如638,942,185</td>

        </tr>
    }
    @if (ViewData["ltruletype"].ToString() == "9")
    {
        <tr>
			  <td colspan="4">游戏总共有0-27共28位数字，0-13为小，14-27为大，奇数为单，偶数为双</td>
			</tr>
			<tr>
			  <td>大小单双：</td>
			  <td align="left">13,14</td>
			  <td>小单</td>
			  <td align="left">包括1,3,5,7,9,11,13,开13回本。</td>
			</tr>
			<tr>
			  <td>大单：</td>
			  <td align="left">15,17,19,21,23,25,27</td>
			  <td>小双</td>
			  <td align="left">0.2.4.6.8.10.12</td>
			</tr>
			<tr>
			  <td>大双：</td>
			  <td align="left">14.16.18.20.22.24.26,开14回本。</td>
			  <td>极小</td>
			  <td align="left">0,1,2,3,4,5</td>
			</tr>
			<tr>
				<td>极大：</td>
				<td align="left">22,23,24,25,26,27</td>
				<td>龙：</td>
				<td align="left">0,3,6,9,12,15,18,21,24,27</td>
			</tr>
			<tr>
				<td>虎：</td>
				<td align="left">1,4,7,10,13,16,19,22,25</td>
				<td>豹：</td>
				<td align="left">2,5,8,11,14,17,20,23,26</td>
			</tr>
    }
    
       @if (ViewData["ltruletype"].ToString() == "13")
       { 
         
       }
    </tbody>
</table>
 
